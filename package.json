  "scripts": {
    "ng": "ng",
    "start": "nodemon dist/server/server.js",
    "watch": "parallelshell \"yarn run watch:client\" \"yarn run watch:server\"",
    "watch:client": "ng build --watch",
    "watch:server": "webpack --config ./config/webpack.server.dev.js --progress --watch",
    "build": "yarn run build:client && yarn run build:server",
    "build:client": "ng build",
    "build:server": "webpack --config ./config/webpack.server.dev.js --progress",
    "build--prod": "yarn run lint && yarn run build--prod:client && yarn run build--prod:server",
    "build--prod:client": "ng build --prod",
    "build--prod:server": "webpack --config ./config/webpack.server.prod.js --progress --bail",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
